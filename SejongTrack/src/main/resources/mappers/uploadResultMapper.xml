<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.ac.sejong.mapper.uploadResultMapper">

    <select id="readrule" resultType="kr.ac.sejong.domain.testVO">
        select
          univ, track, basic, applied, industry
        from
          tbl_rule
        where track = #{trackName}
    </select>

    <select id="readsub" resultType="kr.ac.sejong.domain.trackSubjectVO">
        <![CDATA[
        select
          subjectNo, trackTitle, courseNum, courseTitle, credit
        from
          tbl_suject
          where trackTitle = #{trackName}
        ]]>
    </select>

</mapper>