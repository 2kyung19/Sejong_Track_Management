<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.ac.sejong.mapper.uploadResultMapper">

    <select id="readrule" resultType="kr.ac.sejong.domain.testVO">
        select
          univ, track, basic, applied, industry
        from
          tbl_rule
        where ruleNo = #{ruleNo}
    </select>

    <select id="readsub" resultType="kr.ac.sejong.domain.trackSubjectVO">
        <![CDATA[
        select
          *
        from
          tbl_subject
          where trackNo = #{trackName}
        ]]>
    </select>

    <select id="readtypesub" parameterType="hashmap" resultType="kr.ac.sejong.domain.trackSubjectVO">
        <![CDATA[
        select
          *
        from
          tbl_subject
          where trackNo = #{trackNo}
          and subtype = #{subtype}
        ]]>
    </select>

</mapper>