<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.ac.sejong.mapper.trackAllMapper">
    <select id="readTrack" resultType="kr.ac.sejong.domain.trackAllVO">
        select
          trackNo, group_concat(courseTitle separator ', ') as trackAll
        from
          tbl_subject
        where univNo = #{univNo}
        GROUP BY trackNo
    </select>
</mapper>